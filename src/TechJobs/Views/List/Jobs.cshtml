<h1>@ViewBag.title</h1>

<h3>@ViewBag.jobs.Count Result(s)</h3>

<hr />
<h3>@ViewBag.jobs.Count Result(s)</h3>
@foreach (Dictionary<string, string> item in ViewBag.jobs)
{
    <table class="job-listing">
        <tbody>
            @foreach (KeyValuePair<string, string> job in item)
            {

                <tr>
                    <td>@job.Key</td>
                    <td>@job.Value</td>
                </tr>
            }
        </tbody>

    </table>


}
